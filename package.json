{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dev-server.cmd",
    "dev:direct": "concurrently \"tsc --watch\" \"nodemon\"",
    "build": "tsc",
    "start": "start-server.cmd",
    "start:direct": "node dist/index.js",
    "stop": "stop-server.cmd",
    "restart": "restart-server.cmd",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "seed": "tsx prisma/seed.ts",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "studio": "prisma studio",
    "bundle:be": "node build-backend-bundle.js",
    "docs:generate": "node generate-docs.js",
    "docs:pdf": "powershell -ExecutionPolicy Bypass -File generate-pdf.ps1",
    "docs:docx": "powershell -ExecutionPolicy Bypass -File generate-docx.ps1",
    "docs:all": "npm run docs:generate && npm run docs:pdf",
    "git:solo": "git add . && git diff --cached --quiet || git commit -m \"Update project\" && git pull origin main --no-rebase && git push origin main",
    "git:team": "git add . && git diff --cached --quiet || git commit -m \"Update project\" && git pull origin main --no-rebase && git push origin $(git branch --show-current)",
    "git:new": "git checkout -b"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@prisma/client": "^6.18.0",
    "@sentry/node": "^10.38.0",
    "@sentry/profiling-node": "^10.38.0",
    "@types/mssql": "^9.1.8",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.9.2",
    "module-alias": "^2.2.3",
    "mssql": "^11.0.1",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/ui": "^4.0.18",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "prisma": "^6.18.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^4.0.18"
  }
}
